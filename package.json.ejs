{
  "name": "<%= name %>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env": "DESC=`npm run desc | tail -n1` node gen.js",
    "desc": "echo 构建时间: `date +'%Y-%m-%d %H:%M:%S'` `git log -n 1 HEAD --pretty=format:'版本: %h 时间: %ad 作者:%an 注释:%s' --date=iso`",
    "serve": "vue-cli-service serve ./src/main.ts",
    "start": "NODE_ENV=test yarn env && yarn serve",
    "dev": "NODE_ENV=development yarn env && yarn serve",
    "production": "NODE_ENV=production yarn env && yarn serve",
    "build": "yarn env && NODE_ENV=production vue-cli-service build --no-clean",
    "post-receive": "yarn build",
    "rollback": "./node_modules/.bin/k8s-rollback production ivy-web",
    "rollback-alpha": "./node_modules/.bin/k8s-rollback alpha ivy-web",
    "rollback-release": "./node_modules/.bin/k8s-rollback release ivy-web",
    "get-versions-release": "./node_modules/.bin/k8s-get-versions release ivy-web",
    "get-versions": "./node_modules/.bin/k8s-get-versions production ivy-web",
    "get-versions-alpha": "./node_modules/.bin/k8s-get-versions alpha ivy-web",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@ivy/fts": "<%= ivyFtsVersion %>",
    "@ivy/scripts": "<%= ivyScriptsVersion %>",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "mint-ui": "^2.2.13",
    "underscore": "^1.9.1",
    "vue": "^2.5.17",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/underscore": "1.8.9",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mockjs": "^1.0.1-beta3",
    "postcss-aspect-ratio-mini": "^0.0.2",
    "postcss-px-to-viewport": "^0.0.3",
    "typescript": "^3.0.0",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
